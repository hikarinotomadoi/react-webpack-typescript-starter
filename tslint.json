{
    "rules": {
        "interface-name": false,
        "ordered-imports": true,
        "jsx-no-lambda": false,
        "object-literal-sort-keys": false,
        "prettier": true,
        "no-var-requires": false,
        "no-empty-interface": false,
        "member-access": false
    },
    "rulesDirectory": ["tslint-plugin-prettier"],
    "extends": [
        "tslint:recommended",
        "tslint-react",
        "tslint-config-prettier"
    ]
}
